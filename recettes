CREATE TABLE "recette_recette" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "title" varchar(200) NOT NULL, "instructions" text NOT NULL, "description" varchar(250) NOT NULL, "cuisson" integer NOT NULL, "preparation" integer NOT NULL, "image" varchar(200) NULL, "contree_id" integer NOT NULL REFERENCES "recette_contree" ("id"));
INSERT INTO `recette_recette` VALUES (307,'Crème mic mac','Faire bouillir le lait. Faire fondre le chocolat avec deux cu à soupe d eau. Mélanger la farine, 3 jaunes d œufs et 2 œufs entiers, y ajouter le lait chaud. Faire chauffer et arrêter au premier bouillon. Ajouter le beurre et mélanger. Faire une meringue italienne avec 2 blanc d oeuf et le sucre en sirop et l incorporer à la crème. Diviser la crème en deux parties égales. Ajouter le chocolat fondu dans une moitié. Ajouter le sucre vanillé dans l autre moitié. Verser en même temps les deux crèmes dans une coupe.','Crème anglaise de 2 couleurs',5,20,'',11);
INSERT INTO `recette_recette` VALUES (308,'Brioche lorraine','La veille, mélanger dans une terrine la farine, le sucre, les œufs, la crème, le sel et la levure délayée dans un peu d eau tiède. Laisser reposer. Le lendemain, saupoudrer la pâte d un peu de farine. Fariner la table, étendre la pâte en lui donnant la forme d un rectangle, y déposer le beurre découpé en petits morceaux. Plier la pâte pour enfermer le beurre, étendre la pâte de nouveau, plier en trois, étendre dans l autre sens, plier de nouveau en trois, renouveler cette opération encore deux fois. Étendre une dernière fois la pâte en un rectangle de 60 x 40 cm. Dans la largeur, rouler le morceau de pâte et tailler des boudins de 5 cm. Poser ces boudins dans un moule a manqué beurré en les espaçant de 2 cm. Laisser gonfler à hauteur du moule et faire cuire à four modéré, 180° à 200° (5/6 au thermostat) pendant 30 min.','Brioche légèrement feuilletée',30,20,'IMG_1028.JPG',3);
INSERT INTO `recette_recette` VALUES (309,'Brioche « simple »','Faire un levain avec 1/3 de la farine la levure délayée dans un peu d eau tiède. Le laisser reposer. Pendant qu il lève, mélanger dans une terrine le reste de la farine, le sucre, les œufs, le beurre, le sel. Y ajouter le levain monté et laisser reposer recouvert d un torchon. Le lendemain, pétrir légèrement et mettre dans un moule à brioches. Laisser monter, dorer et enfourner. Cuisson : 180°C 30-45 min ','Brioche de base',40,10,'brios3.jpg',11);
INSERT INTO `recette_recette` VALUES (310,'Nougat Glacé','CONFECTIONNER LA NOUGATINE Sécher au four doux les fruits secs, cela limitera le risque de cristallisation du sucre cuit. Cuire le sucre au stade \"caramel blond\" (sucre + 1/4 du poids en eau + le jus de citron). Mélanger les fruits secs passés au four, enrober du sucre cuit, verser sur un marbre légèrement huilé ou sur du papier de cuisson, retourner pendant la phase de solidification-refroidissement. La nougatine est prête à être broyée quand elle peut être tenue entre les doigts. Hacher grossièrement encore tiède et réserver. LA MERINGUE ITALIENNE AU MIEL CUIT Cuire le miel au grand filet. La production de mousse est normale, spécifique au miel qui cuit ... Verser sur les blancs débrouillés. Contrôler la température. Au fur et à mesure de la montée des blancs la température refroidit. On obtient un développement spectaculaire et une tenue sans pareil. L appareil refroidi (tiédi) ---- Monter la crème comme une chantilly ---- ASSEMBLAGE DE TOUS LES ELEMENTS Incorporer les deux masses en douceur. Le but est de garder un appareil très aéré... Puis on y incorpore les fruits confits macérés et la nougatine broyée très grossièrement. On peut remplir les moules prévus et on lisse sans tasser (attention à ne pas crever les bulles d air emprisonnées!). Placer au grand froid Démouler prestement par un bref passage au bain-marie chaud Puis décorer selon votre inspiration. Apporter de la brillance avec un simple caramel cuit','Trouvé à cette adresse: http://www.chefsimon.com/ngl.htm',0,30,'ngl22.jpg',11);
